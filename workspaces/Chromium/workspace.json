{
  "friendly_name": "Own Chromium",
  "image_src": "chromium.png",
  "description": "Chromium is a free and open-source browser, primarily developed and maintained by Google.",
  "cores": 2,
  "memory": 2768,
  "gpu_count": 0,
  "cpu_allocation_method": "Inherit",
  "docker_registry": "https://index.docker.io/v1/",
  "categories": [
    "Browser"
  ],
  "require_gpu": false,
  "enabled": true,
  "image_type": "Container",
  "run_config": {
    "hostname": "kasm"
  },
  "exec_config": {
    "go": {
      "cmd": "bash -c '/dockerstartup/custom_startup.sh --go --url \"$KASM_URL\"'"
    },
    "assign": {
      "cmd": "bash -c '/dockerstartup/custom_startup.sh --assign --url \"$KASM_URL\"'"
    }
  },
  "architecture": [
    "amd64",
    "arm64"
  ],
  "compatibility": [
    {
      "version": "1.16.x",
      "image": "kasmweb/chromium:1.16.1-rolling-daily",
      "uncompressed_size_mb": 2643,
      "available_tags": [
        "develop",
        "1.16.0",
        "1.16.0-rolling-weekly",
        "1.16.0-rolling-daily",
        "1.16.1",
        "1.16.1-rolling-weekly",
        "1.16.1-rolling-daily"
      ]
    }
  ]
}
