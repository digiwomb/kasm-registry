{
  "friendly_name": "KUbuK",
  "image_src": "kubuk.png",
  "description": "Ubuntu desktop with KDE for Kasm Workspaces, which has been expanded to include useful programs, e.g. for software developers.",
  "cores": 3,
  "memory": 5120,
  "gpu_count": 0,
  "cpu_allocation_method": "Inherit",
  "docker_registry": "https://index.docker.io/v1/",
  "name": "digiwomb/kubuk",
  "categories": [
    "Desktop"
  ],
  "require_gpu": false,
  "enabled": true,
  "image_type": "Container",
  "run_config": {
    "security_opt": [
      "seccomp=unconfined"
    ],
    "entrypoint": [
      "/kasminit"
    ],
    "user": 1000,
    "environment": {
      "HOME": "/home/kasm-user"
    }
  },
  "exec_config": {},
  "architecture": [
    "amd64"
  ],
  "compatibility": [
    "1.13.x",
    "1.14.x",
    "1.15.x",
    "1.16.x"
  ]
}
